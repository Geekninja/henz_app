.row
  .col-md-3
    .mini-stat.clearfix
      %span.mini-stat-icon.orange
        %i.fa.fa-gavel
      .mini-stat-info
        %span R$ 0,00
        Contas à pagar
  .col-md-3
    .mini-stat.clearfix
      %span.mini-stat-icon.tar
        %i.fa.fa-tag
      .mini-stat-info
        %span R$ 0,00
        Contas à receber
  .col-md-3
    .mini-stat.clearfix
      %span.mini-stat-icon.pink
        %i.fa.fa-money
